!!!5
%html
  %head
    %title= title
    = stylesheet_link_tag '//cdnjs.cloudflare.com/ajax/libs/foundation/5.3.3/css/foundation.min.css'
    = stylesheet_link_tag 'application'
  
  %body
    = yield
    = javascript_include_tag '//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.1/jquery.min.js'
    = angular_js_include_tags %w[route resource]
    = javascript_include_tag '//cdn.rawgit.com/angular-ui/ui-utils/v0.1.1/ui-utils.min.js'
    = javascript_include_tag '//maps.googleapis.com/maps/api/js?libraries=places'
    = javascript_include_tag '//connect.facebook.net/en_US/sdk.js'
    = javascript_include_tag 'application'
    #fb-root
